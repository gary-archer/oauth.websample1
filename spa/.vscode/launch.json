{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch SPA",
            "url": "http://localhost/spa/",
            "webRoot": "${workspaceRoot}/src/*",
            "sourceMaps": true,
            "sourceMapPathOverrides":{
                "webpack:///./*": "${workspaceRoot}/dist/*"
            }
        }
    ]
}
